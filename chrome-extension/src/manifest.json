{
  "manifest_version": 3,
  "name": "Qorexal",
  "version": "1.0.0",
  "description": "AI Bot Trader",
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "permissions": [
    "storage",
    "activeTab",
    "identity"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "action": {
    "default_title": "Minimal Angular Extension",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "index.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "https://chatgpt.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.chatgpt.com/*"
      ],
      "js": [
        "content/runtime.js",
        "content/polyfills.js",
        "content/main.js"
      ],
      "css": [
        "content/styles.css"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}